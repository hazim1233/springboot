package com.example.demo;

public class Person {
    private Long id;
    private String ime;
    private int godine;

    // Getteri i setteri
    public Long getId() {
        return id;
    }
    public void setId(Long id) {
        this.id = id;
    }

    public String getIme() {
        return ime;
    }
    public void setIme(String ime) {
        this.ime = ime;
    }

    public int getGodine() {
        return godine;
    }
    public void setGodine(int godine) {
        this.godine = godine;
    }
}
